{
	"name": "faran/pulse",
	"type": "library",
	"version": "1.0.0",
	"autoload": {
		"psr-4": {
			"Faran\\Pulse\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\": "tests/"
		}
	},
	"authors": [
		{
			"name": "Faran Ali",
			"email": "faranaliahmed@gmail.com"
		}
	],
	"minimum-stability": "stable",
	"require": {
		"php": "^8.2",
		"symfony/console": "^7.0"
	},
	"require-dev": {
		"pestphp/pest": "^4.3"
	},
	"bin": [
		"bin/pulse"
	],
	"scripts": {
		"test": "pest",
		"test:coverage": "pest --coverage --min=85"
	},
	"config": {
		"allow-plugins": {
			"pestphp/pest-plugin": true
		}
	}
}
